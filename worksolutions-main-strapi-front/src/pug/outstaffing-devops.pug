        benefitCardHeadlineTag
    )
    +reviews("Отзывы")
    +outstaffServices("Наши услуги по DevOps разработке", servicesItemsText)
    +approachToWork(customServices)
    +selectionOfSpecialists(customSelectionCards)
    +devopsMethodology()
    +ourPartners
    +outstaffPrices("DevOps", outstaffPricesContext)
    +outstaffSeeAlso(developmentTypes)
