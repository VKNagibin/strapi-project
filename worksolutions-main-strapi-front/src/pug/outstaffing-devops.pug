        "Внедрение Docker и Kubernetes",
        "Управление мощностями",
        "Автоматизированное тестирование",
        "Автоматизация развертывания",
        "Управление облачными данными",
        "Настройка CI/CD",
        "Управление безопасностью",
    ];

    var customServices = [
        {
            title: "Усиливаем вашу команду",
            desc: "Войдем в проект командой или одним разработчиком, интегрируемся с вашими менеджерами, разработчиками и другими специалистами."
        },
        {
            title: "Разрабатываем нетиповые проекты на DevOps",
            desc: "Разберемся в инфраструктуре и предметной области. Учитываем особенности и требования бизнеса."
        },
        {
            title: "Используем подход Time & Materials",
            desc: "Вы оплачиваете только фактически затраченное время специалистов над проектом. Оплата производится ежемесячно по факту выполненных работ."
        }
    ];

    var customSelectionCards = [
        {
            title: "Анали&shy;зируем сложность задач",
            position: "1",
            isLast: false,
        },
        {
            title: "Подбираем специалистов на DevOps по стеку и опыту",
            position: "2",
            isLast: false,
        },
        {
            title: "Назначаем дату собеседования",
            position: "3",
            isLast: false,
        },
        {
            title: "Согласо&shy;вываем детали сотрудничества",
            position: "4",
            isLast: false,
        },
        {
            title: "Вводим специалиста на проект",
            position: "5",
            isLast: true,
        },
    ];

    var developmentTypes = [
        { name: "Vue.js разработка", link: "/outstaffing-vue/" },
        { name: "Django разработка", link: "/outstaffing-django/" },
        { name: "Symfony разработка", link: "/outstaffing-symfony/" },
        { name: "Yii разработка", link: "/outstaffing-yii/" },
    ];

    var outstaffPricesContext = {
        minWage: "1 800",
    }

+structure(
    "Аутстаффинг DevOps разработчиков - аренда программистов | Компания «Work Solutions»",
    description,
    "Контакты, Work Solutions, Телефон, Адрес, Почта, Рабочие решения",
    "",
    {
        footer: true
    },
    openGraphMeta
)
    +firstScreenOutstaffing(titleFirst)
    +certificationDevelopers("Сертифика&shy;ция разработчиков")
    +outstaffBenefits(
