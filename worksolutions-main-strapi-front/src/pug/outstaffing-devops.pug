    var pathJs = "outstaffing";
    var pathCss = "outstaffing";
    var ogUrlSite = process.env.OG_URL;

    var description = "Вам нужен аутстаффинг DevOps? Можете смело доверить свой проект нам. 32 сотрудника! 12 лет на рынке! Узкая специализация!";
