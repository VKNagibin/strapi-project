        type: "article",
        locale: "ru_RU",
        description: description,
        image: ogUrlSite + "/i/og/og-image.png",
        url: ogUrlSite + "/outstaffing-devops/",
        siteName: "Work Solutions"
    };

    var titleFirst = [
        { text: "аутстаффинг", highlighted: false },
        { text: "разработчиков", highlighted: false },
        { text: "на DevOps", highlighted: false },
    ];

    var benefitCardHeadlineTag = "h3";

    var servicesItemsText = [
