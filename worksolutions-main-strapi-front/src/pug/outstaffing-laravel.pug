include includes/_

+structure(
  'Аутстаффинг разработчиков - аренда программистов Laravel | Компания «Work Solutions»',
  description,
  'Контакты, Work Solutions, Телефон, Адрес, Почта, Рабочие решения',
  '',
  {
    footer: true
  },
  openGraphMeta
)
  +firstScreenOutstaffing(titleFirst)
  +certificationDevelopers('Сертифика&shy;ция разработчиков')
  +outstaffBenefits('Преимущес&shy;тва аутстаф&shy;финга backend разработ&shy;ки в компании «Work solutions»')
  +reviews('Отзывы')
  +outstaffServices("Наши услуги по Laravel разработке" ,servicesItemsText)
  +approachToWork
  +selectionOfSpecialists
  p {{team_template}}
  +ourPartners
  +outstaffPrices('laravel')
  +outstaffSeeAlso(developmentTypes)
